<p id="notice"><%= notice %></p>

<h1>Lista Clientes</h1>

<table>
  <thead>
    <tr>
      <th>Cliente</th>
      <th>Correo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @lista_clientes.each do |lista_cliente| %>
      <tr>
        <td><%= lista_cliente.cliente %></td>
        <td><%= lista_cliente.correo %></td>
      </tr>
    <% end %>
  </tbody>
</table>

